            <thead>
              <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>D.O.B.</th>
                <th>Address</th>
                <th>Gender</th>
                <th>Course</th>
                <th>Qualification</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td><%= d.name %></td>
                <td><%= d.email %></td>
                <td><%= d.phone %></td>
                <td><%= d.dob %></td>
                <td><%= d.address %></td>
                <td><%= d.gender %></td>
                <td><%= d.course %></td>
                <td><%= d.Qualification %></td>
              </tr>
            </tbody>
            <tfoot>
              <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>D.O.B.</th>
                <th>Address</th>
                <th>Gender</th>
                <th>Course</th>
                <th>Qualification</th>
              </tr>
            </tfoot>
          </table>
        </div>
        <!-- /.card-body -->
      </div>
      <!-- /.card -->
    </div>
      <center><a href="/admin/courseDiasplay" class="btn btn-sm text-white" style="background-color: orangered;"><b>Back To Display</b></a></center>
